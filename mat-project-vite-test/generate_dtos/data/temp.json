{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Evaluate task request",
    "type": "object",
    "properties": {
        "data": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "exercises": {
                    "type": "array",
                    "items": {
                        "oneOf": [
                            {
                                "$schema": "http://json-schema.org/draft-07/schema#",
                                "title": "Doplnovacka evaluate request",
                                "type": "object",
                                "properties": {
                                    "content": {
                                        "type": "array",
                                        "items": {
                                            "oneOf": [
                                                {
                                                    "type": "integer",
                                                    "minimum": 0
                                                },
                                                {
                                                    "type": "string"
                                                }
                                            ]
                                        },
                                        "minItems": 1
                                    }
                                }
                            },
                            {
                                "$schema": "http://json-schema.org/draft-07/schema#",
                                "title": "HledaniChyb evaluate request",
                                "type": "object",
                                "properties": {
                                    "content": {
                                        "type": "string"
                                    }
                                }
                            }
                        ]
                    }
                }
            },
            "required": [
                "exercises"
            ]
        }
    },
    "required": [
        "data"
    ]
}